{
  "manifest_version": 2,

  "name": "WCAG Color Contrast Analyser",
  "description": "Color Contrast Analyser for WCAG 2.0 compliance",
  "version": "1.0",

  "background":
  {
      "scripts": ["jquery-1.11.3.min.js", "ColorContrast.js", "ContextMenu.js", "popup.js"]
  },
  "browser_action": {
    "default_icon": "Images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", "storage"
  ],
  "web_accessible_resources": [
    "font/*.woff2"
  ]
}